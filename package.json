{
  "name": "servicedesk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run && playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:watch": "playwright test --watch",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "build:analyze": "ANALYZE=true next build",
    "init-db": "tsx scripts/init-db.ts",
    "test-db": "tsx scripts/test-db.ts",
    "db:seed": "tsx -e \"import { seedDatabase } from './lib/db'; seedDatabase();\"",
    "db:clear": "tsx -e \"import { clearDatabase } from './lib/db'; clearDatabase();\"",
    "migrate": "tsx lib/db/migrate.ts",
    "migrate:status": "tsx lib/db/migrate.ts status",
    "migrate:run": "tsx lib/db/migrate.ts migrate",
    "migrate:rollback": "tsx lib/db/migrate.ts rollback",
    "env:validate": "tsx -e \"import { validateEnvironment } from './lib/config/env'; validateEnvironment();\"",
    "predev": "npm run env:validate",
    "prebuild": "npm run env:validate",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT .next",
    "sentry:sourcemaps:client": "sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT .next/static/chunks",
    "sentry:sourcemaps:server": "sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT .next/server",
    "sentry:release": "sentry-cli releases new $SENTRY_RELEASE && sentry-cli releases set-commits $SENTRY_RELEASE --auto && sentry-cli releases finalize $SENTRY_RELEASE",
    "sentry:deploy": "npm run build && npm run sentry:sourcemaps && npm run sentry:release",
    "postbuild": "node scripts/sentry-upload-sourcemaps.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@reactflow/background": "^11.3.14",
    "@reactflow/controls": "^11.2.14",
    "@reactflow/core": "^11.11.4",
    "@reactflow/minimap": "^11.7.14",
    "@reactflow/node-resizer": "^2.2.14",
    "@sentry/nextjs": "^8.0.0",
    "@stackframe/stack": "^2.8.39",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bull": "^4.10.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.11",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^11.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.6.0",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dd-trace": "^5.69.0",
    "framer-motion": "^12.23.22",
    "fuse.js": "^7.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.4.3",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "next-auth": "5.0.0-beta.25",
    "nodemailer": "^7.0.6",
    "openai": "^4.104.0",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "react-quill": "^2.0.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.2.1",
    "redis": "^5.8.2",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "happy-dom": "^19.0.2",
    "msw": "^2.11.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
